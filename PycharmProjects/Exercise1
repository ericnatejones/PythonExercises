num1= int(raw_input('Enter your first number'))
num2= int(raw_input('Enter your second number'))

print num1
print num2
print type(num1)
sum=int(num1+num2)
print "The sum of your numbers is:", + sum
if num1>=num2:
    difference=int(num1-num2)
else:
    difference=int(num2-num1)

print "the difference of your numbers is:", + difference

product=int(num1*num2)
print "the product of your numbers is:", + product
if num2==0:
    print "please input a number other than 0 if you want a quotient"
else:
    remainder=num1%num2
    quotient=(num1-remainder)/num2

    print "the quotient of is:", quotient, "with a remainder of", remainder


    print type(quotient)